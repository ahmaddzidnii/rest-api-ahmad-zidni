generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cities {
  city_id   Int     @id
  city_name String?
  prov_id   Int?
}

model districts {
  dis_id   Int     @id
  dis_name String?
  city_id  Int?
}

model postalcode {
  postal_id   Int  @id
  subdis_id   Int?
  dis_id      Int?
  city_id     Int?
  prov_id     Int?
  postal_code Int?
}

model provinces {
  prov_id    Int     @id
  prov_name  String?
  locationid Int?    @db.SmallInt
  status     Int?
}

model subdistricts {
  subdis_id   Int     @id
  subdis_name String?
  dis_id      Int?
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model postalcodeall {
  province    String @id
  city        String?
  district    String?
  subdistrict String?
  postal_code Int?

}
